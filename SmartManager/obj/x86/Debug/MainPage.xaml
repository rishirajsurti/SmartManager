        <TextBlock x:ConnectionId='11' x:Name="sleepTextBlock" HorizontalAlignment="Left" Margin="189,210,0,0" TextWrapping="Wrap" Text="00:00:00" VerticalAlignment="Top" Height="43" Width="161" FontSize="32" TextAlignment="Center" Foreground="White"/>
