<Page
    x:Class="SmartManager.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SmartManager"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Page.Resources>
        <Style TargetType="Button">
            <Setter Property="Background" Value="#4183d7"/>
            <Setter Property="Height" Value="133" />
            <Setter Property="Width" Value="167.5" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="Foreground" Value="White" />
            <!-- <Setter /> -->
        </Style>
    </Page.Resources>
    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" Margin="0,-68,0,68">
        <Button x:ConnectionId='1' x:Name="classesBtn" Content="&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Classes" HorizontalAlignment="Left" Margin="10,69,0,0" VerticalAlignment="Top"                      />
        <Button x:ConnectionId='2' x:Name="codingBtn" Content="&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Coding" HorizontalAlignment="Left" Margin="185,69,0,0" VerticalAlignment="Top"                     />
        <Button x:ConnectionId='3' x:Name="fitnessBtn" HorizontalAlignment="Left" Margin="10,207,0,0" VerticalAlignment="Top" Content="&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Fitness"                      />
