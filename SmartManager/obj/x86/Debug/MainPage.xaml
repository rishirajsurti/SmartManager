        <Button x:ConnectionId='5' x:Name="entertainmentBtn" HorizontalAlignment="Left" Margin="10,345,0,0" VerticalAlignment="Top" Content="&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Entertainment"                  />
        <Button x:ConnectionId='6' x:Name="socialBtn" HorizontalAlignment="Left" Margin="185,345,0,0" VerticalAlignment="Top" Content="&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;Social"                     />
        <TextBox x:ConnectionId='7' x:Name="activity" HorizontalAlignment="Left" Margin="10,483,0,0" TextWrapping="Wrap" Text="Status" VerticalAlignment="Top" Width="340"                                   BorderBrush="{x:Null}" TextAlignment="Center"/>
        <TextBlock x:ConnectionId='8' x:Name="classesTextBlock" HorizontalAlignment="Left" Margin="10,69,0,0" TextWrapping="Wrap" Text="00:00:00" VerticalAlignment="Top" Height="43" Width="161" FontSize="32" TextAlignment="Center" Foreground="White"/>
        <TextBlock x:ConnectionId='9' x:Name="codingTextBlock" HorizontalAlignment="Left" Margin="176,69,0,0" TextWrapping="Wrap" Text="00:00:00" VerticalAlignment="Top" Height="43" Width="174" FontSize="32" TextAlignment="Center" Foreground="White"/>
