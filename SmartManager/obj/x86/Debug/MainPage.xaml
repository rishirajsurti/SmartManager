        	<Run Text=""/>
        </TextBlock>
        <TextBlock x:ConnectionId='16' x:Name="classesPc" HorizontalAlignment="Left" Margin="10,117,0,0" TextWrapping="Wrap" Text="0%" VerticalAlignment="Top" Height="43" Width="161" FontSize="32" TextAlignment="Center"                                                           Foreground="White"/>
