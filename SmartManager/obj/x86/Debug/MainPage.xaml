        <TextBlock x:ConnectionId='14' x:Name="entPc" HorizontalAlignment="Left" Margin="10,393,0,0" TextWrapping="Wrap" Text="0%" VerticalAlignment="Top" Height="43" Width="161" FontSize="32" TextAlignment="Center" RenderTransformOrigin="0.592,5.089" Foreground="White"/>
        <TextBlock x:ConnectionId='15' x:Name="socialPc" HorizontalAlignment="Left" Margin="189,388,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="43" Width="161" FontSize="32" TextAlignment="Center" RenderTransformOrigin="1.441,6.075" Foreground="White">
        	<Run Text="0%"/>
