        </TextBlock>
        <TextBlock x:ConnectionId='20' x:Name="textBlock" HorizontalAlignment="Left" Margin="10,12,0,0" TextWrapping="Wrap" Text="Smart Time Manager" VerticalAlignment="Top" Width="340" Height="52"                                               FontSize="32" FontWeight="Bold"/>
        <HyperlinkButton x:ConnectionId='21' Content="About" HorizontalAlignment="Left" Margin="93,518,0,0" VerticalAlignment="Top" Width="175"                              />

    </Grid>
</Page>

