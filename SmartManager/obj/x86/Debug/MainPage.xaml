        	<LineBreak/>
        	<Run Text=""/>
        </TextBlock>
        <TextBlock x:ConnectionId='19' x:Name="sleepPc" HorizontalAlignment="Left" Margin="189,250,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="43" Width="161" FontSize="32" TextAlignment="Center" Foreground="White">
        	<Run Text="0%"/>
        	<LineBreak/>
